@model IEnumerable<IMS2.Models.Duration>

@{
    ViewBag.Title = "跨度";
}

<h2>跨度</h2>

<p>
    <div class="btn">@Html.ActionLink("添加", "Create")</div>
</p>

<table class="table table-bordered table-hover table-responsive">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DurationName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Remarks)
            </th>
            @*<th>
                操作
            </th>*@
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>
                    <a href="@Url.Action("Details", new { id = item.DurationId })">
                        @Html.DisplayFor(modelItem => item.DurationName)
                    </a>
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Remarks)
                </td>
                @*<td>
                    <a href="@Url.Action("Edit", new { id = item.DurationId })"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>编辑</a> |
                    @Html.ActionLink("详情", "Details", new { id = item.DurationId })
                </td>*@
            </tr>
        }
    </tbody>
</table>
