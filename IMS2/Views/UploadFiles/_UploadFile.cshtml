@model IMS2.ViewModels.UploadFileViews.UploadFileView
@{
    string actionName = ViewBag.ActionName as string;
    string controllerName = ViewBag.ControllerName as string;
}
<h2>@ViewBag.ImportExcelName</h2>
<div class="col-md-12">
    @*@using (Ajax.BeginForm(actionName, controllerName,null, new AjaxOptions { HttpMethod = "Post", InsertionMode = InsertionMode.Replace, LoadingElementId = "import-ajax", UpdateTargetId = "upload-file-review" }, new { enctype = "multipart/form-data", @class = "uploadfileform" }))*@
@using (Html.BeginForm(actionName, controllerName, FormMethod.Post, new { enctype = "multipart/form-data", @class = "uploadfileform" }))
{
        @Html.AntiForgeryToken()

        <div class="form-horizontal">           
            <div class="form-group">
                @Html.LabelFor(model => model.ReporterDate, new { @class = "label-control col-md-2" })
                <div class="col-md-10">
                    @Html.EditorFor(model => model.ReporterDate, new { htmlAttributes = new { @class = "form-control datetimepicker2" } })
                    @Html.ValidationMessageFor(model => model.ReporterDate, "", new { @class = "text-danger" })
                </div>
            </div>
            <div class="form-group">
                <label class="label-control col-md-2">
                    上传文件
                </label>
                <div class="col-md-10">
                    <input type="file" id="inputfileupload" name="files" multiple />

                </div>
            </div>
            <div class="col-md-offset-5 col-md-7">
                <button type="submit" class="btn btn-primary" onclick="UpdateBeginAlert">上传文件</button>
            </div>
        </div>
    }
    
</div>
<div id="upload-file-review" class="col-md-12">

</div>
